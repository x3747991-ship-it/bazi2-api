{
    "$schema": "https://zeabur.com/schema.json",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "deploy": {
        "healthCheck": {
            "path": "/health",
            "timeout": 10,
            "interval": 30
        },
        "startupProbe": {
            "path": "/health",
            "initialDelaySeconds": 30,
            "periodSeconds": 10,
            "timeoutSeconds": 5,
            "failureThreshold": 6
        }
    },
    "env": {
        "PORT": "8000"
    }
}